decorate Capybara::Chromedriver::Logger::Message do
  def extract_file_and_location!
    match = message.match(/^(.+)\s+?(\d+:\d+)\s+?(.+)$/)

    return unless match

    _, @file, @location, _message = match.to_a
  end

  def error?
    super || message =~ /Error:/ || message =~ /Uncaught/
  end
end
